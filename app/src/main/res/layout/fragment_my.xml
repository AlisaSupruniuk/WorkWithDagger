<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".ui.fragment_with_custom_view.FragmentOne"
    tools:viewBindingIgnore="false">


    <by.alisa.supruniuk.workwithdagger.ui.custom_view.MyCustomView
        android:id="@+id/myCustomView"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:layout_gravity="center"
        android:layout_marginTop="100dp">

    </by.alisa.supruniuk.workwithdagger.ui.custom_view.MyCustomView>

    <ProgressBar
        android:id="@+id/pb"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:indeterminateDrawable="@drawable/spinner_png"
        android:indeterminateOnly="true"
        android:visibility="visible" />

    <Button
        android:id="@+id/btnGenerate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginTop="10dp"
        android:text="Generate"
        android:textColor="#191515"
        app:backgroundTint="#83B2B8" />


</LinearLayout>